package com.clearminds.test;
import com.clearminds.conceptos.Calculadora;
import com.clearminds.entidades.Auto;
import com.clearminds.entidades.Persona;

public class TestCalculadora {

	public static void main(String[] args) {
		Calculadora c = new Calculadora();
		
		int resultado = c.sumar(5, 8);
		System.out.println("resultado suma : " + resultado);
		
		double resultado1 = c.restar(10,2);
		System.out.println("resultado resta: " + resultado1);
		
		double resultado2 = c.multiplicar(2.5, 2.5);
		System.out.println("resultado multiplicación: " + resultado2);
		
		c.imprimir();
		
		Persona p;
		Auto a;
	}

}
