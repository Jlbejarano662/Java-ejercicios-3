package com.cmc.excepciones;

public class Ejercicio6 {

	public static void main(String[] args) {
		Cuenta c = new Cuenta();
		try {
			String a = null;
			a.toString();
			c.depositar(-1200);
		} catch (CheckedException e) {
			System.out.println(e.getMessage());
		} catch (Exception e) {
			System.out.println("Sistema no disponible, contacte con 99999");
		}
	}
}


