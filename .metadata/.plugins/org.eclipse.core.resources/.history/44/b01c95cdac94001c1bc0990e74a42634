package com.cmc.excepciones;

import javax.sql.rowset.spi.TransactionalWriter;

public class Cuenta {
	
	private double saldo;
	
	public void depositar(double monto) throws CheckedException {
		if (monto < 0) {
			throw new CheckedException("Monto es incorrecto");
		}
	}
	
	public void depositar(double monto) throws CheckedException {
		if (monto < 0) {
			throw new CheckedException("Monto es incorrecto");
		}
	}
	
}
